{
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"lib": ["es6", "dom", "esnext"],
		"outDir": "./lib",
		"baseUrl": "src",
		"allowJs": true,
		"declaration": true,
		"sourceMap": true,
		"strict": false,
		"removeComments": true,
		"moduleResolution": "Node",
		"noImplicitAny": false,
		"noEmit": false
	},
	"include": ["src/**/*.ts"],
	"exclude": ["node_modules", "lib"]
}
